{
  "fhirServer": {
    "serverUrl": "http://127.0.0.1:5000",
    "authentication": "None"
  },
  "dataLakeStore": {
    "storageUrl": "UseDevelopmentStorage=true"
  },
  "job": {
    "containerName": "test",
    "startTime": "2000-09-01",
    "endTime": "2000-10-01",
    "resourceTypeFilters": [
      "Patient",
      "Observation"
    ]
  },
  "arrow": {
    "readOptions": {
      "useThreads": true,
      "blockSize": 30485760
    },
    "writeOptions": {
      "writeBatchSize": 100,
      "compression": "SNAPPY"
    }
  }
}